{
    "name": "root",
    "private": true,
    "type": "module",
    "scripts": {
        "format": "prettier ./frontend --write",
        "build": "npm run build --workspace=frontend",
        "build_dev": "npm run build_dev --workspace=frontend",
        "build_test": "npm run build_test --workspace=frontend",
        "start": "npm run start --workspace=frontend",
        "lint": "npm run lint --workspace=frontend",
        "test": "jest --config=jest.config.ts",
        "unused-i18-keys": "npx -y tsx ./bin/find-unused-i18-keys.ts",
        "unused-exports": "ts-unused-exports ./frontend/tsconfig.json --maxIssues=0 --showLineNumber --excludePathsFromReport='src/declarations;frontend/src/api/cmc/cmc.did*.*'"
    },
    "workspaces": [
        "frontend"
    ],
    "browserslist": [
        "last 2 chrome version",
        "last 2 firefox version",
        "last 2 safari version",
        "last 2 edge version"
    ],
    "dependencies": {
        "@ant-design/icons": "6.0.0",
        "@dfinity/agent": "2.4.1",
        "@dfinity/auth-client": "2.4.1",
        "@dfinity/candid": "2.4.1",
        "@dfinity/identity": "2.4.1",
        "@dfinity/ledger-icp": "2.6.13",
        "@dfinity/ledger-icrc": "2.9.1",
        "@dfinity/principal": "2.4.1",
        "@dfinity/utils": "2.13.2",
        "antd": "5.26.1",
        "buffer": "6.0.3",
        "clsx": "2.1.1",
        "copy-to-clipboard": "3.3.3",
        "pubsub-js": "1.9.5",
        "query-string": "9.2.2",
        "react-error-boundary": "5.0.0",
        "react-json-tree": "0.20.0",
        "react-router-dom": "6.30.0"
    },
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "overrides": {
        "vite-plugin-node-polyfills": {
            "vite": "6.3.6"
        }
    },
    "devDependencies": {
        "@eslint/compat": "1.3.2",
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.33.0",
        "@types/jest": "30.0.0",
        "@types/node": "22.10.7",
        "@types/pubsub-js": "1.8.6",
        "@types/react": "18.3.18",
        "@types/react-dom": "18.3.5",
        "@typescript-eslint/eslint-plugin": "8.39.1",
        "@typescript-eslint/parser": "8.39.1",
        "@vitejs/plugin-react": "5.0.0",
        "eslint": "9.33.0",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "5.2.0",
        "jest": "30.0.5",
        "jest-environment-jsdom": "30.0.5",
        "prettier": "3.6.2",
        "prettier-plugin-organize-imports": "4.2.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "ts-jest": "29.4.0",
        "ts-node": "10.9.2",
        "typescript": "5.7.3",
        "vite": "6.3.6",
        "vite-plugin-checker": "0.10.2",
        "vite-plugin-environment": "1.1.3",
        "vite-plugin-eslint": "1.8.1",
        "vite-plugin-node-polyfills": "0.24.0"
    }
}
