{
    "name": "root",
    "private": true,
    "type": "module",
    "scripts": {
        "format": "prettier ./frontend --write",
        "build": "npm run build --workspace=frontend",
        "build_dev": "npm run build_dev --workspace=frontend",
        "build_test": "npm run build_test --workspace=frontend",
        "start": "npm run start --workspace=frontend",
        "lint": "npm run lint --workspace=frontend",
        "test": "npm run test --workspace=frontend",
        "unused-i18-keys": "npx -y tsx ./bin/find-unused-i18-keys.ts",
        "unused-exports": "ts-unused-exports ./frontend/tsconfig.json --maxIssues=0 --showLineNumber --excludePathsFromReport='src/declarations;frontend/src/api/cmc/cmc.did*.*'"
    },
    "workspaces": [
        "frontend"
    ],
    "browserslist": [
        "last 2 chrome version",
        "last 2 firefox version",
        "last 2 safari version",
        "last 2 edge version"
    ],
    "dependencies": {
        "@ant-design/icons": "6.1.0",
        "@dfinity/agent": "3.4.3",
        "@dfinity/auth-client": "3.4.3",
        "@dfinity/candid": "3.4.3",
        "@dfinity/identity": "3.4.3",
        "@dfinity/ledger-icp": "8.0.1",
        "@dfinity/ledger-icrc": "6.0.1",
        "@dfinity/nns": "11.1.4",
        "@dfinity/principal": "3.4.3",
        "@dfinity/utils": "4.0.1",
        "antd": "5.28.1",
        "buffer": "6.0.3",
        "clsx": "2.1.1",
        "copy-to-clipboard": "3.3.3",
        "pubsub-js": "1.9.5",
        "query-string": "9.3.1",
        "react-error-boundary": "5.0.0",
        "react-json-tree": "0.20.0",
        "react-router-dom": "6.30.3"
    },
    "engines": {
        "node": ">=24",
        "npm": ">=11.5.1 <12.0.0"
    },
    "devDependencies": {
        "@eslint/compat": "1.4.1",
        "@eslint/eslintrc": "3.3.1",
        "@eslint/js": "9.39.1",
        "@types/node": "24.10.1",
        "@types/pubsub-js": "1.8.6",
        "@types/react": "18.3.18",
        "@types/react-dom": "18.3.5",
        "@typescript-eslint/eslint-plugin": "8.46.4",
        "@typescript-eslint/parser": "8.46.4",
        "@vitejs/plugin-react": "5.1.1",
        "esbuild": "0.27.0",
        "eslint": "9.39.1",
        "eslint-plugin-no-commented-code": "1.0.10",
        "eslint-plugin-react": "7.37.5",
        "eslint-plugin-react-hooks": "7.0.1",
        "jsdom": "27.2.0",
        "prettier": "3.6.2",
        "prettier-plugin-organize-imports": "4.2.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "vite": "6.4.1",
        "vite-plugin-checker": "0.11.0",
        "vite-plugin-environment": "1.1.3",
        "vite-plugin-eslint": "1.8.1",
